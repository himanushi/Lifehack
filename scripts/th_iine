javascript:(function(){const MAX_EXECUTIONS=1243,minSleep=1000,maxSleep=7000;let executions=0;function sleep(ms){return new Promise(r=>setTimeout(r,ms));}function getNthParent(e,n){while(n--&&e){e=e.parentElement;}return e;}function getLikedElements(){return document.querySelectorAll('svg[aria-label="「いいね！」を取り消す"]');}function getUnlikedElement(){return document.querySelector('svg[aria-label="「いいね！」"]');}function gaussianRandom(){let u=0,v=0;while(u===0)u=Math.random();while(v===0)v=Math.random();let num=Math.sqrt(-2.0*Math.log(u))*Math.cos(2.0*Math.PI*v);num=num/10.0+0.5;if(num>1||num<0)return gaussianRandom();return num;}function getRandomSleepTime(){const range=maxSleep-minSleep;return minSleep+Math.floor(gaussianRandom()*range);}async function iine(){if(executions>=MAX_EXECUTIONS){alert("Max number of executions reached.");return;}Array.from(getLikedElements()).forEach(zumi=>{const target=getNthParent(zumi,10);if(target)target.remove();});const unliked=getUnlikedElement();if(unliked)unliked.parentElement.click();await sleep(getRandomSleepTime());executions++;iine();}iine();})();
