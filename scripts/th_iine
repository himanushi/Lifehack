javascript:(function(){const MAX_EXECUTIONS=100;const minSleep=1000;const maxSleep=3000;let executions=0;function sleep(ms){return new Promise(resolve=>setTimeout(resolve,ms));}function getNthParent(e,n){while(n--&&e){e=e.parentElement;}return e;}function getLikedElements(){return document.querySelectorAll('svg[aria-label="「いいね！」を取り消す"][fill="rgb(255, 48, 64)"]');}function getUnlikedElement(){return document.querySelector('svg[aria-label="いいね！"][fill="transparent"]');}function getRandomSleepTime(){return Math.floor(Math.random()*(maxSleep-minSleep+1))+minSleep;}async function iine(){if(executions>=MAX_EXECUTIONS){alert("Max number of executions reached.");return;}Array.from(getLikedElements()).forEach(zumi=>{const target=getNthParent(zumi,10);if(target)target.remove();});const unliked=getUnlikedElement();if(unliked)unliked.parentElement.click();await sleep(getRandomSleepTime());executions++;iine();}iine();})();
